<div>
  <p>Trip Name</p>
  <%= form.text_field :title, placeholder: "Trip Name" %>
</div>
<div>
  <p>Description</p>
  <%= form.text_area :description, placeholder: "Description" %>
</div>
<div>
  <p>Location</p>
  <%= form.text_field :location, placeholder: "Location" %>
</div>
<div>
  <p>Start Date</p>
  <%= form.text_field :start_date, placeholder: "YY/MM/DD", :class => "calendar" %>
</div>
<div>
  <p>End Date</p>
  <%= form.text_field :end_date, placeholder: "YY/MM/DD", :class => "calendar" %>
</div>